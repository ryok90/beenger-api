{
  "name": "beenger-api",
  "license": "MIT",
  "version": "0.0.1",
  "repository": "https://github.com/ryok90/beenger-api",
  "author": "Rodrigo Yokota",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "NODE_PATH=./dist node dist/main/server.js",
    "dev": "cross-env NODE_PATH=./src NODE_ENV=dev ts-node-dev --respawn --transpile-only src/main/server.ts",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:ci": "test --coverage",
    "up": "docker-compose up -d",
    "down": "docker-compose down"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "express": "^4.17.1",
    "mongoose": "^6.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "faker": "^5.5.3",
    "jest": "^27.1.1",
    "joi": "^17.4.2",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  }
}
