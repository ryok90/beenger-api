{
  "name": "beenger-api",
  "version": "0.0.1",
  "repository": "https://github.com/ryok90/beenger-api",
  "author": "Rodrigo Yokota",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main/server.ts",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:ci": "test --coverage",
    "migration-create": "typeorm migration:create -n",
    "seeds": "ts-node ./node_modules/typeorm/cli.js migration:run --config src/infra/db/postgres/ormconfig-seeds.ts"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "express": "^4.17.1",
    "ts-node-dev": "^1.1.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^27.1.1",
    "joi": "^17.4.2",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
